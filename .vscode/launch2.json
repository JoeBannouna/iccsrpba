{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://asrmm71/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Listen for XDebug",
      "type": "php",
      "request": "launch",
      "port": 80
    },
    {
      "name": "Launch currently open script",
      "type": "php",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "port": 80
    }
  ]
}